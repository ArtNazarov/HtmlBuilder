  object sqlCounter: TSQLQuery
    FieldDefs = <>
    Database = conn
    Transaction = trans
    SQL.Strings = (
      'select'
      'content.section,'
      'count(*) AS cnt,'
      'preset.id as preset_id'
      'from content'
      'INNER JOIN'
      #9'section'
      'ON'
      'section.id = content.section'
      'LEFT JOIN'
      #9'preset'
      'ON'
      'section.preset = preset.id'
      'GROUP BY content.section'
      'order by content.section'
    )
    Params = <>
    Macros = <>
    Left = 856
    Top = 768
  end
  object sqlRubrication: TSQLQuery
    FieldDefs = <>
    Database = conn
    Transaction = trans
    SQL.Strings = (
      ' select'
      #9'section.id,'
      '        section.section,'
      '        section.note,'
      '        section.full_text,'
      '        section.tree,'
      #9'content.id as content_id,'
      '        content.caption,'
      '        content.dt,'
      #9'preset.ufn1,'
      #9'content.uf1,'
      #9'preset.ufn2,'
      #9'content.uf2,'
      #9'preset.ufn3,'
      #9'content.uf3,'
      #9'preset.ufn4,'
      #9'content.uf4,'
      #9'preset.ufn5,'
      #9'content.uf5,'
      #9'preset.ufn6,'
      #9'content.uf6,'
      #9'preset.ufn7,'
      #9'content.uf7,'
      #9'preset.sectiontpl,'
      '        preset.headtpl,'
      '        preset.itemtpl,'
      '        preset.dirpath,'
      '        preset.orf,'
      '        preset.ors'
      ''
      ' from section'
      ''
      ''
      'INNER JOIN'
      #9'content'
      'ON'
      'section.id = content.section'
      'LEFT JOIN'
      #9'preset'
      'ON section.preset = preset.id'
      ''
      'WHERE'
      '     section.id = :section_id'
      'ORDER BY {orf} {ors}'
      'LIMIT :pagelimit OFFSET :pageoffset'
    )
    Params = <    
      item
        DataType = ftUnknown
        Name = 'section_id'
        ParamType = ptInput
      end    
      item
        DataType = ftUnknown
        Name = 'pagelimit'
        ParamType = ptInput
      end    
      item
        DataType = ftUnknown
        Name = 'pageoffset'
        ParamType = ptInput
      end>
    Macros = <>
    Left = 101
    Top = 640
