      'preset.ufn5,'
      'content.uf5,'
      'preset.ufn6,'
      'content.uf6,'
      'preset.ufn7,'
      'content.uf7'
      ' from content'
      'LEFT JOIN section'
      'ON content.section = section.id'
      'LEFT JOIN preset'
      'ON section.preset = preset.id'
    )
    Params = <>
    Macros = <>
