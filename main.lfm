    FieldDefs = <>
    Database = conn
    Transaction = trans
    SQL.Strings = (
      'SELECT'
      'content.id,'
      'content.caption,'
      'content.content,'
      'content.dt,'
      'section.id as section_id,'
      'section.section,'
      'section.tree,'
      'preset.sitename,'
      'preset.dirpath,'
      'preset.headtpl,'
      'preset.bodytpl,'
